   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true">
         <image>
            <Image url="@../../../../images/oceanBackground.jpg" />
         </image>
      </ImageView>
      <Pane fx:id="_gamePane" prefHeight="600.0" prefWidth="800.0" />
      <Label fx:id="_timeLabel" layoutX="50.0" layoutY="50.0" styleClass="timer" text="Time : 0s" />
   </children>
</AnchorPane>
